{
    "name": "mtvs/eloquent-approval",
    "description": "Approval process for Laravel Eloquent models.",
    "keywords": ["laravel", "eloquent", "approval"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammad Ali Tavassoli",
            "email": "mtvs.dev@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.3|^9.0|^10.0|^11.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Mtvs\\EloquentApproval\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mtvs\\EloquentApproval\\Tests\\": "tests/",
            "Mtvs\\EloquentApproval\\Tests\\Database\\Factories\\": "tests/database/factories/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtvs\\EloquentApproval\\ApprovalServiceProvider"
            ]
        }
    }
}
